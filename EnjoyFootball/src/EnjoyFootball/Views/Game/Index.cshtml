@model GameDetails

<div>
    <p>plan: @Model.Field</p>
    <p>lediga platser: @Model.OpenSlots</p>
    <p>tid: @Model.StartTime</p>
    <p>aktiv: @Model.IsActive</p>
    <p>@Model.Id</p>
    <p>
        Spelare: @foreach (var item in Model.PlayerList)
        {
        <p>@item.Nickname</p>
        }</p>
    </div>
    <input id="joinGame" type="button" value="Join Game" onclick="location.href='@Url.Action("Addplayer", "Game" , new {gameDetalisId = Model.Id})'" />

