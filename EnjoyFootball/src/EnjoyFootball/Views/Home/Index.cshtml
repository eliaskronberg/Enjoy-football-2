@model List<Match>

<table>
    <tr>
        <th>Location</th>
        <th>Open Slots</th>
        <th>Owner</th>
        <th>Time of match</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td><a href="game/index/@item.Id/">@item.Location</a></td>
            <td>@item.OpenSlots</td>
            <td>@item.Owner</td>
            <td>@item.TimeOfMatch</td>

        </tr>
    }
</table>
